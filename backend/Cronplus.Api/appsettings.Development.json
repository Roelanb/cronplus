{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    }
  },
  "DatabaseSettings": {
    "ConnectionString": "DataSource=cronplus-dev.db",
    "EnableWalMode": true,
    "CreateIfNotExists": true
  },
  "CronplusSettings": {
    "EnableDetailedErrors": true,
    "EnableSwagger": true
  }
}